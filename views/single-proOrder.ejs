<%- include('include/_headerAdmin') %>


<div class="col-md-12" style="height:80px;"></div>
<div class="content">
    <div class="container mt-2 mb-5">
        <div class="row">

            <div class="col-md-12 text-center">
                <a href="/dashboard-proOrders" class="btn mb-3">جميع الطلبات</a>
            </div>
            
            <% for(var i = 0; i < products.length ; i++) { %>
                
                <div class="col-md-4 mb-3">
                    <div class="card p-3">
                        <div class="col-md-12 text-center">

                                <h3 class="mt-4 mb-4"><%= products[i].proname %></h3>
                                <div class="col-md-12 ">
                                    <img src="/<%= products[i].proimage %>" width="100" height="100">
                                    <p> الكميه : <%= products[i].qty %></p>
                                </div>
                            
                        </div>
                    </div>
                </div>
                
            <% } %>
        </div>
    </div>
</div>

<%- include('include/_footerAdmin') %> 